; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="FoxDesktopBuddy"
run/main_scene="res://Scenes/Main.tscn"
config/features=PackedStringArray("4.2", "GL Compatibility")
boot_splash/bg_color=Color(0.141176, 0.141176, 0.141176, 0)
boot_splash/image="res://Images/Logo.png"
boot_splash/fullsize=false
config/icon="res://Images/Logo.png"
config/windows_native_icon="res://Images/Logo.ico"

[display]

window/size/viewport_width=128
window/size/viewport_height=128
window/size/resizable=false
window/size/borderless=true
window/size/always_on_top=true

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
